{
	"extends": ["plugin:prettier/recommended", "plugin:node/recommended"],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"eqeqeq": ["error", "always"],
		"no-empty-function": "error",
		"no-implicit-coercion": "error",
		"prettier/prettier": "error",
		"no-unused-vars": "error"
	}
}
